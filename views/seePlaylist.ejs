<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <table>
        <tr>
            <th>Name Playlist</th>
            <th>List of Song</th>
            <th>Created By</th>
            <th>Actions</th>
        </tr>
        <% data.Playlists.forEach(el => { %>
            <tr>
                <td><%= el.name %></td>
                <td><%= el.Song.title %></td>
                <td><%= data.name %></td>
                <td><a href="/song">Add Song to Playlist</a></td>
            </tr>
         <% }) %>
    </table>
</body>

</html>